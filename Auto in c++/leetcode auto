class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
        string s1 , s2 ; 
      for(auto n : word1) 
        s1 += n ; 
      for(auto j : word2) 
        s2 += j ;
      
      if(s1==s2) 
        return true ; 
      else 
        return false ; 
    }
};
